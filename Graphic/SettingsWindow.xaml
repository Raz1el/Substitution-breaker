        <Label x:Name="Sample371Label" PreviewMouseLeftButtonUp="Sample371Label_OnPreviewMouseLeftButtonUp" Content="37.1" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" Height="29" Canvas.Left="10" Canvas.Top="141" Width="52" FontWeight="Bold"/>
